{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"printWidth": 1000,
	"proseWrap": "never",
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"parser": "json"
			}
		},
		{
			"files": "*.js",
			"options": {
				"parser": "babel"
			}
		},
		{
			"files": "*.css",
			"options": {
				"parser": "css"
			}
		}
	]
}
